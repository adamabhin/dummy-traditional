<?xml version="1.0" encoding="UTF-8"?>

<project>
<modelVersion>4.0.0</modelVersion>
 <groupId>org.alterra.id</groupId>
    <artifactId>loket.net</artifactId>
    <version>1</version>
<build>
  <plugins>
    <plugin>
   <groupId>org.sahli.asciidoc.confluence.publisher</groupId>
   <artifactId>asciidoc-confluence-publisher-maven-plugin</artifactId>
     <version>0.10.0</version>
      <configuration>
        <asciidocRootFolder>Loket.net</asciidocRootFolder>
        <sourceEncoding>UTF-8</sourceEncoding> 
        <rootConfluenceUrl>https://alterra.atlassian.net/wiki</rootConfluenceUrl>
        <skipSslVerification></skipSslVerification>
        <maxRequestsPerSecond></maxRequestsPerSecond>
        <spaceKey>DIV</spaceKey>
        <ancestorId>102727740</ancestorId>
        <username>${username}</username> 
        <password>${password}</password> 
        <orphanRemovalStrategy>KEEP_ORPHANS</orphanRemovalStrategy>
      </configuration>
    </plugin>
    </plugins>
</build>
</project>
